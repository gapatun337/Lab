[32m+[m[32mdef task2():[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    print("–ü—Ä–∏–≤–µ—Ç! –ü—Ä–∏—Å—Ç—É–ø–∞—é –∫ —Ä–µ—à–µ–Ω–∏—é –∑–∞–¥–∞–Ω–∏—è N2")[m[41m[m
[32m+[m[32m    # –±–ª–æ–∫ –≤–≤–æ–¥–∞ –¥–∞–Ω–Ω—ã—Ö[m[41m[m
[32m+[m[32m    x = float(input("–í–≤–µ–¥–∏ x: "))[m[41m[m
[32m+[m[32m    # –±–ª–æ–∫ –≤—ã—á–∏—Å–ª–µ–Ω–∏—è[m[41m[m
[32m+[m[32m    a = -2[m[41m[m
[32m+[m[32m    b = -2[m[41m[m
[32m+[m[32m    x_result = (a * ((x ** b + x) / 3) - (x ** b) * (1 / 3))[m[41m[m
[32m+[m[32m    # –±–ª–æ–∫ –≤—ã–≤–æ–¥–∞ –¥–∞–Ω–Ω—ã—Ö[m[41m[m
[32m+[m[32m    print('–û—Ç–≤–µ—Ç: x = {:.4f}. –°–ø–∞—Å–∏–±–æ, —á—Ç–æ –≤–æ—Å–ø–æ–ª—å–∑–æ–≤–∞–ª—Å—è –º–Ω–æ–π!'.format(x_result))[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mtask2()[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport math[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mdef task3():[m[41m[m
[32m+[m[32m    print('–ü—Ä–∏–≤–µ—Ç! –ü—Ä–∏—Å—Ç—É–ø–∞—é –∫ —Ä–µ—à–µ–Ω–∏—é –∑–∞–¥–∞–Ω–∏—è N3')[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    x = float(input('–≤–≤–µ–¥–∏ x: '))[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    base = 3[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    x_result = (math.log(math.cosh(x ** 1 / 3), base))[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    print('–û—Ç–≤–µ—Ç: x = {:.4f}. –°–ø–∞—Å–∏–±–æ, —á—Ç–æ –≤–æ—Å–ø–æ–ª—å–∑–æ–≤–∞–ª—Å—è –º–Ω–æ–π!'.format(x_result))[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mtask3()[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport math[m[41m[m
[32m+[m[41m[m
[32m+[m[32mdef task4(radius=None, distance=None):[m[41m[m
[32m+[m[32m    print('–ü—Ä–∏–≤–µ—Ç! –ü—Ä–∏—Å—Ç—É–ø–∞—é –∫ —Ä–µ—à–µ–Ω–∏—é –∑–∞–¥–∞–Ω–∏—è N4')[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    if radius is None:[m[41m[m
[32m+[m[32m        radius = float(input("–í–≤–µ–¥–∏—Ç–µ —Ä–∞–¥–∏—É—Å: "))[m[41m[m
[32m+[m[32m    if distance is None:[m[41m[m
[32m+[m[32m        distance = float(input("–í–≤–µ–¥–∏—Ç–µ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ: "))[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    section_area = calculate_section_area(radius, distance)[m[41m[m
[32m+[m[32m    print("–ü–ª–æ—â–∞–¥—å —Å–µ—á–µ–Ω–∏—è —à–∞—Ä–∞:", section_area)[m[41m[m
[32m+[m[41m    [m
[32m+[m[32mdef calculate_section_area(radius, distance):[m[41m[m
[32m+[m[32m    if distance >= radius:[m[41m[m
[32m+[m[32m        return 0[m[41m[m
[32m+[m[32m    else:[m[41m[m
[32m+[m[32m        section_radius = math.sqrt(radius**2 - distance**2)[m[41m[m
[32m+[m[32m        section_area = math.pi * section_radius**2[m[41m[m
[32m+[m[32m        return section_area[m[41m[m
[32m+[m[41m    [m
[32m+[m[32mtask4()[m[41m   [m
[32m+[m[41m[m
[32m+[m[32mdef task5():[m[41m[m
[32m+[m[32m    print('–ü—Ä–∏–≤–µ—Ç! –ü—Ä–∏—Å—Ç—É–ø–∞—é –∫ —Ä–µ—à–µ–Ω–∏—é –∑–∞–¥–∞–Ω–∏—è N5')[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    m1 = float(input('–≤–≤–µ–¥–∏ m1: '))[m[41m[m
[32m+[m[32m    v1 = float(input('–≤–≤–µ–¥–∏ v1: '))[m[41m[m
[32m+[m[32m    m2 = float(input('–≤–≤–µ–¥–∏ m2: '))[m[41m[m
[32m+[m[32m    v2 = float(input('–≤–≤–µ–¥–∏ v2: '))[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    v = (m1 * v1 + m2 * v2) / (m1 + m2)[m[41m[m
[32m+[m[32m    v1_after = 2 * v - v1[m[41m[m
[32m+[m[32m    v2_after = 2 * v - v2[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    print("–°–∫–æ—Ä–æ—Å—Ç—å –¥–≤–∏–∂–µ–Ω–∏—è —Ç–µ–ª–µ–∂–µ–∫ –ø–æ—Å–ª–µ —Å–æ—É–¥–∞—Ä–µ–Ω–∏—è: {:.2f} –º/—Å".format(v))[m[41m[m
[32m+[m[32m    print("–°–∫–æ—Ä–æ—Å—Ç—å –ø–µ—Ä–≤–æ–π —Ç–µ–ª–µ–∂–∫–∏ –ø–æ—Å–ª–µ —Å–æ—É–¥–∞—Ä–µ–Ω–∏—è: {:.2f} –º/—Å".format(v1_after))[m[41m[m
[32m+[m[32m    print("–°–∫–æ—Ä–æ—Å—Ç—å –≤—Ç–æ—Ä–æ–π —Ç–µ–ª–µ–∂–∫–∏ –ø–æ—Å–ª–µ —Å–æ—É–¥–∞—Ä–µ–Ω–∏—è: {:.2f} –º/—Å".format(v2_after))[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mtask5()[m[41m[m
[32m+[m[41m[m
[32m+[m[32mimport math[m[41m[m
[32m+[m[41m[m
[32m+[m[32mdef task6():[m[41m[m
[32m+[m[32m    print('–ü—Ä–∏–≤–µ—Ç! –ü—Ä–∏—Å—Ç—É–ø–∞—é –∫ —Ä–µ—à–µ–Ω–∏—é –∑–∞–¥–∞–Ω–∏—è N6')[m[41m[m
[32m+[m[41m[m
[32m+[m[32mdef calculate_prism_surface_area(radius, height):[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    cylinder_circumference = 2 * math.pi * radius[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    prism_surface_area = cylinder_circumference * height[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    return prism_surface_area[m[41m[m
[32m+[m[41m[m
[32m+[m[32mtask6()[m[41m[m
[32m+[m[41m[m
[32m+[m[32mradius = float(input("–í–≤–µ–¥–∏—Ç–µ —Ä–∞–¥–∏—É—Å –æ—Å–Ω–æ–≤–∞–Ω–∏—è —Ü–∏–ª–∏–Ω–¥—Ä–∞: "))[m[41m[m
[32m+[m[32mheight = float(input("–í–≤–µ–¥–∏—Ç–µ –≤—ã—Å–æ—Ç—É —Ü–∏–ª–∏–Ω–¥—Ä–∞: "))[m[41m[m
[32m+[m[41m[m
[32m+[m[32msurface_area = calculate_prism_surface_area(radius, height)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mprint("–ü–ª–æ—â–∞–¥—å –±–æ–∫–æ–≤–æ–π –ø–æ–≤–µ—Ä—Ö–Ω–æ—Å—Ç–∏ –ø—Ä–∏–∑–º—ã:", surface_area)[m[41m[m
[32m+[m[41m[m
[32m+[m[32mdef task7():[m[41m[m
[32m+[m[32m    print('–ü—Ä–∏–≤–µ—Ç! –ü—Ä–∏—Å—Ç—É–ø–∞—é –∫ —Ä–µ—à–µ–Ω–∏—é –∑–∞–¥–∞–Ω–∏—è N7')[m[41m[m
[32m+[m[41m    [m
[32m+[m[32m    def solve_quadratic_equation(A, B, C):[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        discriminant = B**2 - 4*A*C[m[41m[m
[32m+[m[41m    [m
[32m+[m[41m        [m
[32m+[m[32m        if discriminant < 0:[m[41m[m
[32m+[m[32m            print("–î–µ–π—Å—Ç–≤–∏—Ç–µ–ª—å–Ω—ã—Ö –∫–æ—Ä–Ω–µ–π –Ω–µ—Ç.")[m[41m[m
[32m+[m[32m        else:[m[41m[m
[32m+[m[41m            [m
[32m+[m[32m            root1 = (-B + math.sqrt(discriminant)) / (2*A)[m[41m[m
[32m+[m[32m            root2 = (-B - math.sqrt(discriminant)) / (2*A)[m[41m[m
[32m+[m[41m    [m
[32m+[m[41m            [m
[32m+[m[32m            if root1 < root2:[m[41m[m
[32m+[m[32m                print("{:.4f}".format(root1))[m[41m[m
[32m+[m[32m                print("{:.4f}".format(root2))[m[41m[m
[32m+[m[32m            else:[m[41m[m
[32m+[m[32m                print("{:.4f}".format(root2))[m[41m[m
[32m+[m[32m                print("{:.4f}".format(root1))[m[41m[m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[32m    A = float(input("–í–≤–µ–¥–∏—Ç–µ –∫–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç A: "))[m[41m[m
[32m+[m[32m    B = float(input("–í–≤–µ–¥–∏—Ç–µ –∫–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç B: "))[m[41m[m
[32m+[m[32m    C = float(input("–í–≤–µ–¥–∏—Ç–µ –∫–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç C: "))[m[41m[m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[32m    if A == 0:[m[41m[m
[32m+[m[32m        print("–ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç A –Ω–µ –º–æ–∂–µ—Ç –±—ã—Ç—å –Ω—É–ª–µ–º.")[m[41m[m
[32m+[m[32m    else:[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        solve_quadratic_equation(A, B, C)[m[41m[m
[32m+[m[41m    [m
[32m+[m[32mtask7()[m[41m[m
[32m+[m[41m[m
[32m+[m[32mdef task8():[m[41m[m
[32m+[m[32m    print('–ü—Ä–∏–≤–µ—Ç! –ü—Ä–∏—Å—Ç—É–ø–∞—é –∫ —Ä–µ—à–µ–Ω–∏—é –∑–∞–¥–∞–Ω–∏—è N8')[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    def calculate_angular_velocity(latitude):[m[41m[m
[32m+[m[32m        # –ü–µ—Ä–µ–≤–æ–¥–∏–º —à–∏—Ä–æ—Ç—É –∏–∑ –≥—Ä–∞–¥—É—Å–æ–≤ –≤ —Ä–∞–¥–∏–∞–Ω—ã[m[41m[m
[32m+[m[32m        latitude_rad = math.radians(latitude)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # –†–∞—Å—á–∏—Ç—ã–≤–∞–µ–º —É–≥–ª–æ–≤—É—é —Å–∫–æ—Ä–æ—Å—Ç—å –≤—Ä–∞—â–µ–Ω–∏—è –ó–µ–º–ª–∏[m[41m[m
[32m+[m[32m        angular_velocity = 2 * math.pi / 24  # 2œÄ / 24 —á–∞—Å–∞[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return angular_velocity[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    def calculate_linear_velocity(radius, latitude):[m[41m[m
[32m+[m[32m        # –†–∞—Å—á–∏—Ç—ã–≤–∞–µ–º —É–≥–ª–æ–≤—É—é —Å–∫–æ—Ä–æ—Å—Ç—å –≤—Ä–∞—â–µ–Ω–∏—è –ó–µ–º–ª–∏[m[41m[m
[32m+[m[32m        angular_velocity = calculate_angular_velocity(latitude)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # –†–∞—Å—á–∏—Ç—ã–≤–∞–µ–º –ª–∏–Ω–µ–π–Ω—É—é —Å–∫–æ—Ä–æ—Å—Ç—å[m[41m[m
[32m+[m[32m        linear_velocity = radius * angular_velocity[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return linear_velocity[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    def calculate_linear_acceleration(radius, latitude):[m[41m[m
[32m+[m[32m        # –†–∞—Å—á–∏—Ç—ã–≤–∞–µ–º —É–≥–ª–æ–≤—É—é —Å–∫–æ—Ä–æ—Å—Ç—å –≤—Ä–∞—â–µ–Ω–∏—è –ó–µ–º–ª–∏[m[41m[m
[32m+[m[32m        angular_velocity = calculate_angular_velocity(latitude)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # –†–∞—Å—á–∏—Ç—ã–≤–∞–µ–º –ª–∏–Ω–µ–π–Ω–æ–µ —É—Å–∫–æ—Ä–µ–Ω–∏–µ[m[41m[m
[32m+[m[32m        linear_acceleration = radius * angular_velocity**2[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        return linear_acceleration[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    # –†–∞–¥–∏—É—Å –ó–µ–º–ª–∏ (–≤ –∫–º)[m[41m[m
[32m+[m[32m    radius_earth = 6370[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    # –í–≤–æ–¥ —à–∏—Ä–æ—Ç—ã —Ç–æ—á–∫–∏[m[41m[m
[32m+[m[32m    latitude = float(input("–í–≤–µ–¥–∏—Ç–µ —à–∏—Ä–æ—Ç—É —Ç–æ—á–∫–∏ –≤ –≥—Ä–∞–¥—É—Å–∞—Ö: "))[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    # –†–∞—Å—á–µ—Ç –ª–∏–Ω–µ–π–Ω–æ–π —Å–∫–æ—Ä–æ—Å—Ç–∏[m[41m[m
[32m+[m[32m    linear_velocity = calculate_linear_velocity(radius_earth, latitude)[m[41m[m
[32m+[m[32m    print("–õ–∏–Ω–µ–π–Ω–∞—è —Å–∫–æ—Ä–æ—Å—Ç—å —Ç–æ—á–∫–∏ –Ω–∞ —à–∏—Ä–æ—Ç–µ {}: {:.2f} –∫–º/—á".format(latitude, linear_velocity))[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    # –†–∞—Å—á–µ—Ç –ª–∏–Ω–µ–π–Ω–æ–≥–æ —É—Å–∫–æ—Ä–µ–Ω–∏—è[m[41m[m
[32m+[m[32m    linear_acceleration = calculate_linear_acceleration(radius_earth, latitude)[m[41m[m
[32m+[m[32m    print("–õ–∏–Ω–µ–π–Ω–æ–µ —É—Å–∫–æ—Ä–µ–Ω–∏–µ —Ç–æ—á–∫–∏ –Ω–∞ —à–∏—Ä–æ—Ç–µ {}: {:.2f} –∫–º/—á^2".format(latitude, linear_acceleration))[m[41m[m
[32m+[m[41m[m
[32m+[m[32mtask8()[m[41m[m
[32m+[m[41m[m
[32m+[m[32mdef task9():[m[41m[m
[32m+[m[32m    print('–ü—Ä–∏–≤–µ—Ç! –ü—Ä–∏—Å—Ç—É–ø–∞—é –∫ —Ä–µ—à–µ–Ω–∏—é –∑–∞–¥–∞–Ω–∏—è N9')[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    exchange_rate = 0.098[m[41m[m
[32m+[m[32m    commission = 0.01[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    amount_yuan = float(input("–í–≤–µ–¥–∏—Ç–µ —Å—É–º–º—É –≤ —é–∞–Ω—è—Ö ¬•: "))[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    amount_rubles = amount_yuan * exchange_rate * (1 - commission)[m[41m[m
[32m+[m[32m    amount_rubles = round(amount_rubles, 2)[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    print("–°—É–º–º–∞ –≤ —Ä—É–±–ª—è—Ö: {:.2f}".format(amount_rubles))[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32mtask9()[m
\ No newline at end of file[m
